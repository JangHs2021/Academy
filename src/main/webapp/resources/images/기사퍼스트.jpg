	var media_slider_module_ = new Swiper(media_slider_, {
		slidesPerView : "auto",
		navigation: {
			nextEl: '#pb-hub-book-001-69Ucu-slider-button .swiper-button-next',
			prevEl: '#pb-hub-book-001-69Ucu-slider-button .swiper-button-prev',
		},
		// centeredSlides: true,
		// loop: true,
		// slidesPerView : 4,
		// slidesPerGroup: 1,
		// paginationClickable: true,
		// spaceBetween: 30
	});

		
		var tab_menu_el_ = $("#pb-hub-book-001-69Ucu-tab");
		var all_slides_ = media_slider_.find(".swiper-slide[data-ctg-id]");

		tab_menu_el_.find("a[data-ctg-id]").click($.proxy(function(event_){
			var ctg_id_ = $(event_.currentTarget).attr("data-ctg-id");
			var isbn_map_ = [];

			var target_slides_ = [];

			this['slides'].each(function(){
				var slide_ = $(this);
				var toggled_ = slide_.attr("data-ctg-id") === ctg_id_;

				slide_.toggle(toggled_);

				if(toggled_) target_slides_.push(slide_);
			});

			//filtering isbn
			var isbn_map_ = [];
			$.each(target_slides_, function(){
				var slide_ = $(this);
				var isbn_ = slide_.attr("data-isbn");
				var toggled_ = (isbn_map_.indexOf(isbn_) < 0);
				isbn_map_.push(isbn_);

				slide_.toggle(toggled_);
			});

			$("#pb-hub-book-001-69Ucu-slider-button").toggle((target_slides_.length > 4));

			setTimeout($.proxy(function(){
				this.update();
			}, this['slider_module']), 50);
		},{
			slides : all_slides_,
			slider_module : media_slider_module_,
		})).first().click();
});

		</script>
