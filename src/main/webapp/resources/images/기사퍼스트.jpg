window._pb_naver_login_callback = function(result_){
	PB.post("naver-login",{
		'naver_data' : result_
	}, function(result_, response_json_){
		if(!result_ || response_json_.success !== true){

			if(response_json_.do_register){

				var sns_register_form_ = $("#pb-sns-register-form");

				sns_register_form_.find("[name='email']").val(result_['email']);
				sns_register_form_.find("[name='name']").val(result_['username']);
				sns_register_form_.find("[name='nick']").val(result_['username']);

				sns_register_form_.find("[name='birth2']").val(result_['birth2']);
				sns_register_form_.find("[name='birth3']").val(result_['birth3']);
				sns_register_form_.find("[name='gender']").val(result_['gender']);

				sns_register_form_.find("[name='sns_id']").val(result_['id']);
				sns_register_form_.find("[name='sns_version']").val("naver");

				sns_register_form_.submit();

			}else{
				PB.alert({
					title : response_json_.error_title || "에러발생",
					content : response_json_.error_message || "로그인 중, 에러가 발행했습니다.",
				});
			}

			

			return;
		}

		location.reload();

	}, true);
}

