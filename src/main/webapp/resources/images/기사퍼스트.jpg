<script type="text/javascript">
	$(document).ready(function(){
		var window_el_ = $(window);
		var body_el_ = $("body");
		var pb_header_ = $("#pb-header");
		var last_scroll_top_ = window_el_.scrollTop();
		var last_scroll_direction_ = "up";
		var scroll_deep_start_top_ = last_scroll_top_;

		if(body_el_.hasClass("with-fixed-header")){
			window_el_.scroll(function(event_){
				var scroll_top_ = window_el_.scrollTop();
				var scroll_direction_ = scroll_top_ > last_scroll_top_ ? "down" : "up";

				body_el_.toggleClass('with-header-scroll-white-fixed', (scroll_top_ >= 200));

				if(scroll_direction_ === last_scroll_direction_){
					var scroll_deep_ = Math.abs(scroll_deep_start_top_ - scroll_top_);
						
					if(scroll_deep_ >= 200){
						body_el_.toggleClass('with-header-scroll-fixed', (scroll_direction_ === "down"));
					}

				}else{
					scroll_deep_start_top_ = scroll_top_;
				}

				last_scroll_top_ = scroll_top_;
				last_scroll_direction_ = scroll_direction_;
			});
			window_el_.scroll();
		}
		

			

		var megamenu_panel_ = $("#pb-mega-menu-panel");

		$("#pb-header-mega-menu-toggle-btn").click(function(){
			var toggle_ = !body_el_.hasClass("with-mega-menu-panel");
			body_el_.toggleClass("with-mega-menu-panel", toggle_);


			if(toggle_){
				megamenu_panel_.toggleClass("end", false);
				setTimeout(function(){
					megamenu_panel_.toggleClass("out", false);
				},30);
			}else{
				megamenu_panel_.toggleClass("out", true);
				setTimeout(function(){
					megamenu_panel_.toggleClass("end", true);
				},500);
			}


			return false;
		});

		// AOS.init();
	});


</script>
