<script type="text/javascript">
jQuery(document).ready(function(){
	var floating_banner_els_ = $("[data-floating-banner]");
	floating_banner_els_.each(function(){
		var floating_banner_el_ = $(this);

		var close_btn_el_ = floating_banner_el_.find("[data-close-btn]");
		var link_el_ = floating_banner_el_.find("a.banner-link");

		link_el_.click(function(){
			if(PBVAR['is_mobile']){
				var target_link_ = $(this);
				document.location = target_link_.attr("data-href-mobile");
				return false;
			}
		});

		close_btn_el_.click(function(){
			$(this).closest("[data-floating-banner]").remove();
			return false;
		});

		floating_banner_el_.find("[data-map-string]").each(function(){
			var target_image_ = $(this);
			var map_string_id_ = target_image_.attr("data-map-string");
			var map_id_ = target_image_.attr("usemap");

			var temp_map_el_ = $('<div></div>');
				temp_map_el_.html($(map_string_id_).html());
			
			$(map_id_).html(temp_map_el_.children("map").html());
			$(map_id_).imageMapResize();
		});
	});
});
</script>

	
<script type="text/javascript">
jQuery(document).ready(function(){
	var strip_banner_els_ = $("[data-strip-banner]");

	strip_banner_els_.each(function(){
		var strip_banner_el_ = $(this);
		var close_btn_el_ = strip_banner_el_.find("[data-close-btn]");
		var link_el_ = strip_banner_el_.find("[data-href-mobile]");

		link_el_.click(function(){
			if(PBVAR['is_mobile']){
				var target_link_ = $(this);
				document.location = target_link_.attr("data-href-mobile");
				return false;
			}
		});

		close_btn_el_.click(function(){
			$(this).closest("[data-strip-banner]").remove();
			_pb_hub_check_strip_banner();
			return false;
		});

		strip_banner_el_.find("[data-map-string]").each(function(){
			var target_image_ = $(this);
			var map_string_id_ = target_image_.attr("data-map-string");
			var map_id_ = target_image_.attr("usemap");

			var temp_map_el_ = $('<div></div>');
				temp_map_el_.html($(map_string_id_).html());
			
			$(map_id_).html(temp_map_el_.children("map").html());
			$(map_id_).imageMapResize();
		});


	});


	_pb_hub_check_strip_banner();
});

jQuery(document).ready(function(){
	setTimeout(function(){
		_pb_hub_check_strip_banner();
	}, 100);

	$(window).resize(_pb_hub_check_strip_banner);

	_pb_hub_check_strip_banner();
});
function _pb_hub_check_strip_banner(){
	var body_el_ = $("body");
	body_el_.toggleClass("with-strip-footer-banner", ($("[data-strip-banner-footer]").length > 0));
	body_el_.toggleClass("with-strip-header-banner", ($("[data-strip-banner-header]").length > 0));

	var strip_banner_el_ = $("[data-strip-banner-header]");

	if(strip_banner_el_.length <= 0){
		var header_ = $("#pb-header");

		header_.css({"top" : ""});
		body_el_.css({"paddingTop": ""});
	}else{
		var header_ = $("#pb-header");

		var header_is_fixed_ = header_.css("position") === "fixed";

		var header_top_ = strip_banner_el_.outerHeight();
		var body_top_padding_ = (header_is_fixed_ ? 0 : header_.outerHeight()) + header_top_;

		header_.css({"top" : header_top_});
		body_el_.css({"paddingTop": body_top_padding_});
	}
}
</script>




<script type="text/javascript">
function pb_view_notice(id_){
	PB.post("template-theme-default-load-notice-modal", {
		post_id : id_
	}, function(result_, response_json_){
		if(!result_ || response_json_.success !== true){
			PB.alert({
				title : "에러발생",
				content : "교재정보를 불러오는 중, 에러가 발생했습니다.",

			});
			return;
		}

		$("#pb-notice-popup").remove();
		$(response_json_.popup_html).appendTo("body");

		var popup_ = $("#pb-notice-popup");

		popup_.modal("show");

	}, true);

}	
</script>

