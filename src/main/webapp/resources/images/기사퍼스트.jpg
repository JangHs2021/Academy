function pb_kakao_login(){
	Kakao.Auth.login({
		success: function(authObj) {
			Kakao.API.request({
				url: '/v2/user/me',
				success: function(res) {

					var kakao_mem = new Array();
					kakao_mem['id'] = res.id;
					kakao_mem['email'] = res.kaccount_email;
					kakao_mem['nickname'] = res.properties.nickname;
					Kakao.API.request({
						url: '/v2/user/me',
						success: function(res2) {
							kakao_mem['birthday'] = res2.birthday;
							kakao_mem['birthdayType'] = res.birthdayType;
						},
						fail: function(error) {
							akakakoSnsLogin(kakao_mem);
						}
					});

					_pb_check_login_for_kakao(kakao_mem);
				},
				fail: function(error) {

					alert(JSON.stringify(err));
				}
			});

		},
		fail: function(err) {

		  alert(JSON.stringify(err));
		}
	});
}	

function _pb_check_login_for_kakao(kakao_data_){
	var request_data_ = {
		id : kakao_data_.id,
		email : kakao_data_.email ? kakao_data_.email : null,
		username : kakao_data_.nickname ? kakao_data_.nickname : null,
		birthday : kakao_data_.birthday ? kakao_data_.birthday : null,
		birthday_type : kakao_data_.birthdayType ? (kakao_data_.birthdayType === "SOLAR" ? "1" : "2") : null,		
	}

	PB.post("kakao-login",{
		'kakao_data' : request_data_
	}, function(result_, response_json_){
		if(!result_ || response_json_.success !== true){

			if(response_json_.do_register){

				var sns_register_form_ = $("#pb-sns-register-form");

				sns_register_form_.find("[name='email']").val(request_data_['email']);
				sns_register_form_.find("[name='name']").val(request_data_['username']);
				sns_register_form_.find("[name='nick']").val(request_data_['username']);

				var birth_m_  ='';
				var birth_d_ = '';
				if(request_data_['birthday']){
					birth_m_  = request_data_['birthday'].substring(0,2);
					birth_d_ = request_data_['birthday'].substring(3,2);
				}

				sns_register_form_.find("[name='birth2']").val(birth_m_);
				sns_register_form_.find("[name='birth3']").val(birth_d_);
				sns_register_form_.find("[name='birth4']").val(request_data_['birthday_type']);

				sns_register_form_.find("[name='sns_id']").val(request_data_['id']);
				sns_register_form_.find("[name='sns_version']").val("kakao");

				sns_register_form_.submit();

			}else{
				PB.confirm({
					title : response_json_.error_title || "에러발생",
					content : response_json_.error_message || "로그인 중, 에러가 발행했습니다.",
				});
			}

			return;
		}

		location.reload();

	}, true);
}

